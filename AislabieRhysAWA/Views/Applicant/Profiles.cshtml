@model IEnumerable<NAA.Data.Profile>

@{
    ViewBag.Title = "Profiles";
}

<h2>Profiles</h2>

<p>
    @Html.ActionLink("Create New", "CreateProfile", new {Controller = "Admin" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApplicantName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicantAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ApplicantName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApplicantAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditProfile", new { id = item.ApplicantId, controller = "Admin" }) |
            @Html.ActionLink("Details", "Details", new { id=item.ApplicantId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ApplicantId, controller = "Admin" })
        </td>
    </tr>
}

</table>
